                        </li>
                    </ul>

                    <% if (user && user.role !== 'admin') { %>
                        <ul class="navbar-nav">
                            <li class="nav-item">
                                <a role="button" href="/auth/logout" class="login-btn btn btn-outline-primary nav-link text-black p-2 mx-5">
                                    Logout

                                    <style>
                                        .login-btn{
                                            border-color: orange;
                                            color: orange;
                                        }
                                        .login-btn:hover {
                                            color: white !important;
                                            background-color: orange !important;
                                            border-color: orange !important;
                                        }
                                    </style>
                                
                                
